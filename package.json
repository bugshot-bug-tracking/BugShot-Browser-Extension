{
	"name": "bugshot-extension",
	"version": "1.2.4",
	"description": "BugShot is an easy-to-use tool to track the bugs on your projects.",
	"displayName": "BugShot",
	"type": "module",
	"scripts": {
		"dev": "clear; rimraf dist/development && cross-env NODE_ENV=development run-p dev:*",
		"dev-chrome": "clear; rimraf dist/development && cross-env NODE_ENV=development run-p dev:chromium",
		"dev:chromium": "cross-env NODE_TARGET=chromium esno scripts/build.ts",
		"dev:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"dev:safari": "cross-env NODE_TARGET=safari esno scripts/build.ts",
		"empty": "",
		"build": "clear; rimraf dist/production && cross-env NODE_ENV=production run-s build:*",
		"build:chromium": "cross-env NODE_TARGET=chromium esno scripts/build.ts",
		"build:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"build:safari": "cross-env NODE_TARGET=safari esno scripts/build.ts"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^2.0.0",
		"@types/chrome": "^0.0.260",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.11.19",
		"@types/webextension-polyfill": "^0.10.7",
		"@unocss/preset-rem-to-px": "^0.58.5",
		"@unocss/reset": "^0.58.5",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vuepic/vue-datepicker": "^3.6.8",
		"@vueuse/core": "^10.8.0",
		"@webcomponents/webcomponentsjs": "^2.8.0",
		"axios": "^1.6.7",
		"chokidar": "^3.6.0",
		"cross-env": "^7.0.3",
		"esno": "^4.0.0",
		"fs-extra": "^11.2.0",
		"kolorist": "^1.8.0",
		"naive-ui": "^2.37.3",
		"npm-run-all": "^4.1.5",
		"pinia": "^2.1.7",
		"rimraf": "^5.0.5",
		"sass": "^1.71.1",
		"tributejs": "^5.1.3",
		"typescript": "^5.3.3",
		"unocss": "^0.58.5",
		"unplugin-auto-import": "^0.17.5",
		"unplugin-vue-components": "^0.26.0",
		"vite": "^5.1.4",
		"vue": "3.4.19",
		"vue-demi": "^0.14.7",
		"vue-i18n": "^9.9.1",
		"vue-tribute": "^2.0.0",
		"vue-universal-modal": "^1.1.4",
		"webext-bridge": "^5.0.5",
		"webextension-polyfill": "^0.10.0"
	}
}
