{
	"name": "bug-shot-extension",
	"version": "1.1.10",
	"description": "BugShot is an easy-to-use tool to track the bugs on your projects.",
	"displayName": "BugShot",
	"scripts": {
		"dev": "clear; rimraf dist/development && cross-env NODE_ENV=development run-p dev:*",
		"dev:chrome": "cross-env NODE_TARGET=chrome esno scripts/build.ts",
		"dev:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"dev:edge": "cross-env NODE_TARGET=edge esno scripts/build.ts",
		"dev:opera": "cross-env NODE_TARGET=opera esno scripts/build.ts",
		"empty": "",
		"build": "clear; rimraf dist/production && cross-env NODE_ENV=production run-s build:*",
		"build:chrome": "cross-env NODE_TARGET=chrome esno scripts/build.ts",
		"build:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"build:edge": "cross-env NODE_TARGET=edge esno scripts/build.ts",
		"build:opera": "cross-env NODE_TARGET=opera esno scripts/build.ts"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^0.9.2",
		"@types/chrome": "^0.0.223",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.15.3",
		"@types/vue-select": "^3.16.2",
		"@types/webextension-polyfill": "^0.10.0",
		"@unocss/preset-rem-to-px": "^0.51.8",
		"@unocss/reset": "^0.51.8",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vuepic/vue-datepicker": "^3.5.2",
		"@vueuse/core": "^9.13.0",
		"@webcomponents/webcomponentsjs": "^2.7.0",
		"axios": "^1.3.4",
		"chokidar": "^3.5.3",
		"cross-env": "^7.0.3",
		"esno": "^0.16.3",
		"fs-extra": "^11.1.0",
		"kolorist": "^1.7.0",
		"naive-ui": "^2.34.3",
		"npm-run-all": "^4.1.5",
		"pinia": "^2.0.33",
		"rimraf": "^4.4.0",
		"sass": "^1.59.2",
		"tributejs": "^5.1.3",
		"typescript": "^4.9.5",
		"unocss": "^0.50.4",
		"unplugin-auto-import": "^0.15.1",
		"unplugin-vue-components": "^0.24.1",
		"vite": "^4.1.4",
		"vue": "3.2.40",
		"vue-demi": "^0.13.11",
		"vue-i18n": "^9.2.2",
		"vue-select": "4.0.0-beta.3",
		"vue-tribute": "^2.0.0",
		"vue-universal-modal": "^1.1.4",
		"webext-bridge": "^5.0.5",
		"webextension-polyfill": "^0.10.0"
	}
}
