{
	"name": "bugshot-extension",
	"version": "1.2.3",
	"description": "BugShot is an easy-to-use tool to track the bugs on your projects.",
	"displayName": "BugShot",
	"scripts": {
		"dev": "clear; rimraf dist/development && cross-env NODE_ENV=development run-p dev:*",
		"dev-chrome": "clear; rimraf dist/development && cross-env NODE_ENV=development run-p dev:chrome",
		"dev:chrome": "cross-env NODE_TARGET=chrome esno scripts/build.ts",
		"dev:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"dev:edge": "cross-env NODE_TARGET=edge esno scripts/build.ts",
		"dev:opera": "cross-env NODE_TARGET=opera esno scripts/build.ts",
		"dev:safari": "cross-env NODE_TARGET=safari esno scripts/build.ts",
		"empty": "",
		"build": "clear; rimraf dist/production && cross-env NODE_ENV=production run-p build:*",
		"build:chrome": "cross-env NODE_TARGET=chrome esno scripts/build.ts",
		"build:firefox": "cross-env NODE_TARGET=firefox esno scripts/build.ts",
		"build:edge": "cross-env NODE_TARGET=edge esno scripts/build.ts",
		"build:opera": "cross-env NODE_TARGET=opera esno scripts/build.ts",
		"build:safari": "cross-env NODE_TARGET=safari esno scripts/build.ts"
	},
	"devDependencies": {
		"@intlify/unplugin-vue-i18n": "^0.12.2",
		"@types/chrome": "^0.0.242",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.17.1",
		"@types/webextension-polyfill": "^0.10.1",
		"@unocss/preset-rem-to-px": "^0.53.6",
		"@unocss/reset": "^0.53.6",
		"@vitejs/plugin-vue": "^4.2.3",
		"@vuepic/vue-datepicker": "^3.6.8",
		"@vueuse/core": "^10.2.1",
		"@webcomponents/webcomponentsjs": "^2.8.0",
		"axios": "^1.4.0",
		"chokidar": "^3.5.3",
		"cross-env": "^7.0.3",
		"esno": "^0.17.0",
		"fs-extra": "^11.1.1",
		"kolorist": "^1.8.0",
		"naive-ui": "^2.34.4",
		"npm-run-all": "^4.1.5",
		"pinia": "^2.1.4",
		"rimraf": "^5.0.1",
		"sass": "^1.64.1",
		"tributejs": "^5.1.3",
		"typescript": "^5.1.6",
		"unocss": "^0.53.6",
		"unplugin-auto-import": "^0.16.6",
		"unplugin-vue-components": "^0.25.1",
		"vite": "^4.4.7",
		"vue": "3.3.4",
		"vue-demi": "^0.14.5",
		"vue-i18n": "^9.2.2",
		"vue-tribute": "^2.0.0",
		"vue-universal-modal": "^1.1.4",
		"webext-bridge": "^5.0.5",
		"webextension-polyfill": "^0.10.0"
	}
}
